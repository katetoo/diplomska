<%= render 'shared/page_title', title: 'Најавете се' %>

<div class="row">
  <div class="well col-md-6 col-md-offset-3">
    <%= form_tag '/login' do %>
      <%= label_tag :Е_маил %>
      <%= text_field_tag :email_korisnik %>
              
      <%= label_tag :Лозинка %>
      <%= password_field_tag :password %>
              
      <%= submit_tag 'Најави се', class: 'btn btn-success' %>
        
    <% end %>
    </div>
</div>